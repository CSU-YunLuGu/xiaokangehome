page {
	background: rgba(255, 255, 255, 1);
}

.certification {
  padding: 35rpx;
  /* box-sizing: border-box; */
  /* min-height: 100vh; */
}

.submit {
  font-family: 'HarmonyOS Sans SC';
  margin-top: 194rpx;
  left: 32rpx;
  top: 194rpx;
  width: 468rpx;
  height: 40rpx;
  opacity: 1;
  font-size: 36rpx;
  font-weight: 500;
  letter-spacing: 0rpx;
  line-height: 40rpx;
  color: rgba(64, 64, 64, 1);
  text-align: left;
  vertical-align: top;
}

.card-cont {
  height: 400rpx;
  display: flex;
  flex-direction: column;
}

.card-list {
  margin: 0 auto;
}
.card-list image{
  width: 324rpx;
  height: 284rpx;
  margin-top: 38rpx;
  margin-left: 16rpx;
  /* display: flex; */
  border-radius: 20rpx;
  justify-content: center;
}

.condition-list {
  font-family: 'HarmonyOS Sans SC';
  margin-top: 54rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: rgba(64, 64, 64, 1);
}

.condition {
  font-size: 35rpx;
  margin-bottom: 20rpx;
}

.condition1 {
  margin-top: 32rpx;
  left: 32rpx;
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(171, 171, 171, 1);
  text-align: left;
  vertical-align: top;
}

.condition2 {
  left: 32rpx;
  margin-top: 16rpx;
}

.style1 {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(171, 171, 171, 1);
}

.style2 {
  font-size: 28rpx;
  font-weight: 500;
  color: rgba(255, 87, 87, 1);
}

.notice {
  margin-top: 36rpx;
  left: 8rpx;
  width: 688rpx;
  height: 174rpx;
}

.upload {
  margin-top: 100rpx;
  /* width: 100%; */
  /* 在.wxss中设置按钮长度无效 */
  height: 88rpx;
  border-radius: 12rpx;
  background: #2972F6;
  opacity: 1;
  /* box-shadow: 0rpx 0rpx 12rpx  rgba(0, 0, 0, 0.1); */
  color: #fff;
  justify-content: center;
}

.upload[disabled] {
  background-color: #66E6FF;
}

.circle .wx-checkbox-input {
  border-radius: 50%;
  width: 24rpx;
  height: 24rpx;
}

.agreement {
  margin-top: 32rpx;
  left: 34rpx;
  font-family: 'HarmonyOS Sans SC';
  font-size: 24rpx;
  font-weight: 400;
}

.style3 {
  color: rgba(171, 171, 171, 1);
}

.style4 {
  color: #576b95; 
  cursor: pointer;
  text-decoration: underline;
}


@font-face {
  font-family: 'HarmonyOS Sans SC';
  src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
      url('data:font/woff;charset=utf-8;base64,') format('woff'),
      url('subset-HarmonyOS_Sans_SC.ttf') format('truetype');
  font-weight: normal;
  font-style: normal;
  font-display: swap;
}