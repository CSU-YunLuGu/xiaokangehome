page {
  background: rgba(244, 244, 244, 1);
}


.box {
  position: absolute;
  left: 50rpx;
  top: 20%;
  width: 650rpx;
  padding-bottom: 30%;
}

.title {
  position: absolute;
  left: 30rpx;
  top: 30rpx;
  font-size: 35rpx;
  font-weight: 600;
}

.analysisbox {
  margin-top: 20rpx;
  display: block;
  width: 650rpx;
  height: 440rpx;
  border-radius: 20rpx;
  background-color: white;
  box-shadow: 0 0 15rpx rgba(0, 0, 0, 0.2);
  position: relative;
}

.consulthistory {
  position: absolute;
  top: 500rpx;
  width: 650rpx;
  padding-bottom: 10%;
}

con {
  display: block;
}

.searchbox {
  width: 650rpx;
  height: 60rpx;
  position: absolute;
  left: 50rpx;
  top: 20%;
  margin-top: 40rpx;
  border-radius: 15rpx;
  background: rgba(255, 255, 255, 1);
  box-shadow: 0px 0px 15rpx rgba(0, 0, 0, 0.2);
}

.input {
  position: absolute;
  left: 20rpx;
  top: 5rpx;
  width: 480rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 25rpx;
}

.submit {
  position: absolute;
  margin-left: 200rpx;
  margin-top: 350rpx;
  width: 250rpx;
  height: 50rpx;
  background-color: rgb(255, 188, 60);
  border-radius: 10rpx;
  color: white;
  font-size: 25rpx;
  line-height: 50rpx;
  text-align: center;
}

textarea {
  position: absolute;
  margin: 50rpx;
  display: block;
  margin-top: 100rpx;
  width: 530rpx;
  height: 200rpx;
  font-size: 25rpx;
  line-height: 40rpx;
  background-color: rgb(255, 188, 60, 0.2);
  border-radius: 10rpx;
  padding: 10rpx;
}

.icon {
  position: absolute;
  left: 600rpx;
  top: 10rpx;
  width: 45rpx;
  height: 45rpx;
}

.iconimg {
  width: 45rpx;
  height: 45rpx;
}